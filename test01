package com.optum.fads.userroles.api.config.dao;

import java.util.List;

import com.optum.fads.userroles.api.config.domain.LuValues;
import com.optum.fads.userroles.api.config.domain.LuValuesId;

public interface LuValuesDAO {

    void save(LuValues instance);

    void delete(LuValues instance);

    LuValues findById(LuValuesId id);

    List<LuValues> findByExample(LuValues instance);

    List<LuValues> findByProperty(String propertyName, Object value);

    List<LuValues> findByLuCode(Object luCode);

    List<LuValues> findByLuLabel(Object luLabel);

    List<LuValues> findByLuView(Object luView);

    List<LuValues> findByObjId(Object objId);

    List<LuValues> findByLuSeq(Object luSeq);

    List<LuValues> findByLuParent(Object luParent);

    List<LuValues> findAll();

    LuValues merge(LuValues detachedInstance);

    void attachDirty(LuValues instance);

    void attachClean(LuValues instance);

    List<LuValues> findByFieldIdAndOrderBySeq(String fieldId);
}
