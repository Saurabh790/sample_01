@ExceptionHandler(StudyNotFoundException.class)
	public final ResponseEntity<StudyApiError> handleStudyAlreadyExistException(Exception ex, WebRequest request) {
		List<String> details= new ArrayList<String>();
		details.add(request.getDescription(false));
		StudyApiError errorDetails = new StudyApiError(LocalDateTime.now(), ex.getMessage(),details);
	//	logger.error(ex.getMessage());
		return new ResponseEntity<>(errorDetails, HttpStatus.NOT_FOUND);
	}
	
