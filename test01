@Test
void getCasesDetailsByJobId_shouldReturnListOfCaseIds() {
    Integer jobId = 10;

    // Arrange
    JobMasterCasesT c1 = new JobMasterCasesT();
    c1.setCaPrmNodeCd("A");
    c1.setCaScndNodeCd("B");
    c1.setCaSequenceId(1);     // MUST NOT be null
    c1.setCaYearId(2025);      // set this too (avoid null surprises)

    JobMasterCasesT c2 = new JobMasterCasesT();
    c2.setCaPrmNodeCd("C");
    c2.setCaScndNodeCd("D");
    c2.setCaSequenceId(2);     // MUST NOT be null
    c2.setCaYearId(2025);

    JobMasterT job = new JobMasterT();
    job.setJobMasterCasesTs(List.of(c1, c2)); // MUST NOT be null

    when(jobsRepository.getJobById(jobId)).thenReturn(job);

    // Act
    List<String> out = service.getCasesDetailsByJobId(jobId);

    // Assert
    assertNotNull(out);
    assertEquals(2, out.size());

    // Optional: verify exact values if you know the format createCaseId uses
    // assertEquals("A-B-1-2025", out.get(0));
    // assertEquals("C-D-2-2025", out.get(1));

    verify(jobsRepository).getJobById(jobId);
}
