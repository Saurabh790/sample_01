	private Table createPreviousAppeal(Table datatable, AppealBean appeal, DeviceRgb selectedColor) {
		try {
			PdfFont font = commonPDFHelper.getCustomFont();
			PdfFont fontBold = commonPDFHelper.getCustomBoldFont();
			
			Text key = new Text(CaseLabel.APPEAL_ID).setFont(fontBold).setFontSize(8);
			Text space = new Text("  ");
			Text value = new Text(String.valueOf(appeal.getAppealId())).setFont(font).setFontSize(8);
			Paragraph appealId = new Paragraph("");

			List textList = new ArrayList();
			textList.add(key);
			textList.add(space);
			textList.add(value);
			appealId.addAll(textList);
			Cell referenceCell = new Cell(0, 2).add(appealId).setFont(font).setFontSize(8);
			referenceCell.setBorder(Border.NO_BORDER).setBackgroundColor(selectedColor);
			datatable.addCell(referenceCell);

			key = new Text(CaseLabel.APPEAL_TYPE).setFont(fontBold).setFontSize(8);
			value = new Text(appeal.getAppealTypeDesc()).setFont(font).setFontSize(8);
			Paragraph appealType = new Paragraph("");

			textList = new ArrayList();
			textList.add(key);
			textList.add(space);
			textList.add(value);
			appealType.addAll(textList);
			Cell dateCell = new Cell(0, 2).add(appealType);
			dateCell.setBorder(Border.NO_BORDER).setBackgroundColor(selectedColor);
			datatable.addCell(dateCell);

			key = new Text(CaseLabel.APPEAL_START_DATE).setFont(fontBold).setFontSize(8);
			value = new Text(appeal.getStartOfAppealDt()).setFont(font).setFontSize(8);
