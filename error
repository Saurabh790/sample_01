if (singleValue.getDrLabel() != null && !singleValue.getDrLabel().isBlank() &&
    singleValue.getId() != null &&
    singleValue.getId().getDrValue() != null && !singleValue.getId().getDrValue().isBlank()) {

    codeBean.setValue(singleValue.getDrLabel());
    codeBean.setCode(singleValue.getId().getDrValue());
}
