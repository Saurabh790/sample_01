package com.optum.fads.casetracking.pdf;

import java.beans.PropertyDescriptor;
import java.util.List;

import com.optum.fads.caseconfig.dto.CaseMetaBaseDto;
import com.optum.fads.caseconfig.dto.LabelValueBean;
import com.optum.fads.casetracking.common.AppealsMetaBaseEnum;
import com.optum.fads.casetracking.common.CaseConstants;
import com.optum.fads.casetracking.util.CaseUtil;
import org.apache.commons.lang.StringUtils;
import org.apache.log4j.Logger;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import com.itextpdf.kernel.color.DeviceRgb;
import com.itextpdf.kernel.font.PdfFont;
import com.itextpdf.layout.border.Border;
import com.itextpdf.layout.element.Cell;
import com.itextpdf.layout.element.Paragraph;
import com.itextpdf.layout.element.Table;
import com.optum.fads.casetracking.appeals.AppealBean;
import com.optum.fads.casetracking.common.CaseLabel;
import com.optum.fads.casetracking.dto.CaseDetailDTO;

@Component
public class AppealsPDF {
	private final Logger logger = Logger.getLogger(this.getClass().getName());
	
	@Autowired
	CommonPDFHelper commonPDFHelper;

	public Table createAppeals(CaseDetailDTO caseDTO, List<AppealBean> prevAppeals, List<CaseMetaBaseDto> customFields) {
		Table appealsTable = new Table(new float[] {1});
		appealsTable.setWidthPercent(100).setFixedLayout();
		Cell cell = new Cell();
		cell.setBorder(Border.NO_BORDER);

		Cell headerCell = new Cell().add(commonPDFHelper.createHeader(caseDTO));
		appealsTable.addCell(headerCell);
		cell = new Cell().add(commonPDFHelper.createCommonGeneralInformation(caseDTO, true));
		appealsTable.addCell(cell);

		Cell appealHdr = commonPDFHelper.createPlainHeader(CaseLabel.MENU_APPEAL_HDR, null);
		appealHdr.setBorder(Border.NO_BORDER);
		appealsTable.addCell(appealHdr);

		cell = new Cell().add(createCurrentAppealData(caseDTO, customFields)).setBorder(Border.NO_BORDER);
		appealsTable.addCell(cell);
		cell = new Cell().add(createPreviousAppealsData(prevAppeals, customFields)).setBorder(Border.NO_BORDER);
		appealsTable.addCell(cell);

		return appealsTable;
	}

	private Table createAppealHeader(String header) {
		Table detailTable = new Table(new float[]{1, 1, 1, 1});
		detailTable.setWidthPercent(100).setFixedLayout();

		PdfFont font = commonPDFHelper.getCustomBoldFont();
		DeviceRgb deviceRgb = new DeviceRgb(194, 86, 8);
		Paragraph caseInfo = new Paragraph(header).setFont(font).setFontSize(10).setFontColor(deviceRgb);
		Cell hcell = new Cell(0, 4).add(caseInfo);
		hcell.setBorder(Border.NO_BORDER);
		detailTable.addCell(hcell);
		return detailTable;
	}

	private Table createCurrentAppealData(CaseDetailDTO caseDTO, List<CaseMetaBaseDto> customFields) {
		Table infoTable = new Table(new float[]{1});
		infoTable.setWidthPercent(100);

		Table detailTable = createAppealHeader(CaseLabel.APPEAL_CURRENT_HDR);
		Cell infoCell = new Cell().add(detailTable);
		infoCell.setBorder(Border.NO_BORDER);
		infoTable.addCell(infoCell);

		AppealBean cAppeal = caseDTO.getCurrentAppeal();
		if (cAppeal != null) {
			infoTable.addCell(createAppeal(cAppeal, customFields, false));
		} else {
			PdfFont font = commonPDFHelper.getCustomFont();
			Paragraph caseInfo = new Paragraph("No current appeal.").setFont(font).setFontSize(8).setMarginRight(25);
			Cell hcell = new Cell(0, 4).add(caseInfo);
			hcell.setBorder(Border.NO_BORDER);
			infoTable.addCell(hcell);
		}

		return infoTable;
	}

	private Table createAppeal(AppealBean appeal, List<CaseMetaBaseDto> customFields, boolean isPrevAppeal) {
		Table detailTable = new Table(new float[]{1, 1, 1, 1});
		detailTable.setWidthPercent(100).setFixedLayout();
		if (isPrevAppeal) {
			detailTable.addCell(commonPDFHelper.createFieldLabel(CaseLabel.APPEAL_ID));
			Cell cell = new Cell(0, 3).add(commonPDFHelper.creatFieldValue(StringUtils.defaultString(String.valueOf(appeal.getAppealId()), "")));
			cell.setBorder(Border.NO_BORDER);
			detailTable.addCell(cell);
		}

		int count = 0;
		for (CaseMetaBaseDto caseMetaBaseDto : customFields) {
			String value = "";
			try {
				if (StringUtils.equals(caseMetaBaseDto.getFieldType(), CaseConstants.CASE_FIELD_TYPE_DECIMAL)) {
					Double dValue = (Double) new PropertyDescriptor(caseMetaBaseDto.getFieldName(), AppealBean.class).getReadMethod().invoke(appeal);
					if (dValue != null) {
						value = "$ " + CaseUtil.getDoubleInString(dValue);
					}
				} else if (StringUtils.equals(caseMetaBaseDto.getFieldType(), CaseConstants.CASE_FIELD_TYPE_NUMBER)) {
					Long dValue = (Long) new PropertyDescriptor(caseMetaBaseDto.getFieldName(), AppealBean.class).getReadMethod().invoke(appeal);
					if (dValue != null) {
						value = commonPDFHelper.formatNumber(String.valueOf(dValue));
					}
				} else if (StringUtils.equals(caseMetaBaseDto.getFieldType(), "CHECKBOX")) {
					Boolean dValue = (Boolean) new PropertyDescriptor(caseMetaBaseDto.getFieldName(), AppealBean.class).getReadMethod().invoke(appeal);
					if (dValue) {
						value = "Y";
					}
				} else if (CaseConstants.CASE_FIELD_TYPE_DROPDOWN.equals(caseMetaBaseDto.getFieldType())) {
					if (AppealsMetaBaseEnum.STATUS_CD.name().equalsIgnoreCase(caseMetaBaseDto.getFieldId())) {
						value = appeal.getAppealStatusDesc();
					} else if (AppealsMetaBaseEnum.TYPE_CD.name().equalsIgnoreCase(caseMetaBaseDto.getFieldId())) {
						value = appeal.getAppealTypeDesc();
					} else if (AppealsMetaBaseEnum.DECISION_CD.name().equalsIgnoreCase(caseMetaBaseDto.getFieldId())) {
						value = appeal.getDecisionAppealDesc();
					} else if (AppealsMetaBaseEnum.ASSIGNED_TO.name().equalsIgnoreCase(caseMetaBaseDto.getFieldId())) {
						value = appeal.getAssignedUserName();
					} else { // add for other scenarios of Cust Text type fields might be changed to Drop-Down type
						value = (String) new PropertyDescriptor(caseMetaBaseDto.getFieldName(), AppealBean.class).getReadMethod().invoke(appeal);
						for (LabelValueBean labelValueBean : caseMetaBaseDto.getFieldValidValues()) {
							if (StringUtils.equals(labelValueBean.getValue(), value)) {
								value = labelValueBean.getLabel();
								break;
							}
						}
					}
				} else {
					value = (String) new PropertyDescriptor(caseMetaBaseDto.getFieldName(), AppealBean.class).getReadMethod().invoke(appeal);
				}

			} catch (Exception e) {
				logger.error("Exception in createCurrentAppealData: ", e);
			}

			if (!"TEXTAREA".equalsIgnoreCase(caseMetaBaseDto.getFieldType())) {
				detailTable.addCell(commonPDFHelper.createFieldLabel(caseMetaBaseDto.getPrettyLabel()));
			}
			if ("TEXTAREA".equalsIgnoreCase(caseMetaBaseDto.getFieldType())) {
					if (count != 0 && count % 2 != 0) {
						Cell blankcell = new Cell();
						blankcell.setBorder(Border.NO_BORDER);
						detailTable.addCell(blankcell);
						detailTable.addCell(blankcell);
					}

				Cell blankcell = new Cell();
				blankcell.setBorder(Border.NO_BORDER);
				detailTable.addCell(commonPDFHelper.createFieldLabel(caseMetaBaseDto.getPrettyLabel()));
				Cell cell = new Cell(0, 3).add(commonPDFHelper.creatFieldValue(CaseUtil.findTrimText(StringUtils.defaultString(value, ""))));
				cell.setBorder(Border.NO_BORDER);
				detailTable.addCell(cell);
			} else {
				if (StringUtils.isBlank(value)) {
					detailTable.addCell(commonPDFHelper.creatFieldValue(""));
				} else {
					detailTable.addCell(commonPDFHelper.creatFieldValue(CaseUtil.findTrimText(StringUtils.defaultString(value, ""))));
				}
			}
			count++;
		}
		return detailTable;
	}

	private Table createPreviousAppealsData(List<AppealBean> prevAppeals, List<CaseMetaBaseDto> customFields) {
		Table infoTable = new Table(new float[] {1});
		infoTable.setWidthPercent(100);
		Table detailTable = createAppealHeader(CaseLabel.APPEAL_PREVIOUS_HDR);
		Cell infoCell = new Cell().add(detailTable);
		infoCell.setBorder(Border.NO_BORDER);
		infoTable.addCell(infoCell);

		if (prevAppeals != null && prevAppeals.size()>0) {
			for (AppealBean appeal : prevAppeals) {
				infoTable.addCell(createAppeal(appeal, customFields, true));
			}
		} else {
			PdfFont font = commonPDFHelper.getCustomFont();
			Paragraph caseInfo = new Paragraph("No previous appeal.").setFont(font).setFontSize(8).setMarginRight(25);
			Cell hcell = new Cell(0, 4).add(caseInfo);
			hcell.setBorder(Border.NO_BORDER);
			infoTable.addCell(hcell);
		}

		return infoTable;
	}

