	@Override
	@Transactional(readOnly = true)
	public StudyDTO getStudy(Long id) {

		Optional<PrmStudyMasterT> optional = studyOwnRepository.findById(id);// .orElseThrow(() -> new
																			// StudyNotFoundException(id)));
		if (optional.isPresent()) {
			PrmStudyMasterT prmStudyMasterT = optional.get();
			StudyDTO studyDTO = studyDetailMapper.toStudyDTO(prmStudyMasterT);
			 List<StudyRsRiItem> studyRsRiItems = studyDetailMapper.toStudyRsRiItems(prmStudyMasterT.getPrmStudyRsRiTs());
		
			studyRsRiItems.forEach(studyRsRiItem -> {
				studyDTO.getReportSections().forEach(rs -> {
					if (rs.getReportSectionId() == studyRsRiItem.getReportSectionId()) {
						rs.getReportItems().forEach(ri -> {
							if (ri.getReportItemId() == studyRsRiItem.getReportItemId()) {
								rs.setRsOrder(studyRsRiItem.getRsOrder());
								ri.setLowerLimit(studyRsRiItem.getLowerLimit());
								ri.setUpperLimit(studyRsRiItem.getUpperLimit());
								ri.setPercentage(studyRsRiItem.getPercentage());
								ri.setStandardDeviation(studyRsRiItem.getStandardDeviation());
								ri.setWeight(studyRsRiItem.getWeight());
							}
						});
					}
				});

			});

			List<StudyRsRiException> studyRsRiExceptions = studyDetailMapper
					.toStudyRsRiExceptions(prmStudyMasterT.getPrmStudyRsRiExceptTs());	
			studyRsRiExceptions.forEach(srx -> {
				studyDTO.getReportSections().forEach(rs -> {
					if (rs.getReportSectionId() == srx.getReportSectionId()) {
						rs.getReportItems().forEach(ri -> {
							if (ri.getReportItemId() == srx.getReportItemId()) {
								 studyDTO.getTimePeriods().forEach(timePeriod ->{
									 if(timePeriod.getTimePeriodId() == srx.getTimePeriodId()) {
									 ri.getExceptionProcesses().add(new ExceptionProcess(srx.getTimePeriodId(),srx.getExceptionFlag(),
													srx.getOverLimitFlag()));
									 }
								 });
							}
							});
					}

				});

			});

		List<ReportSection> rsSortedList = studyDTO.getReportSections().stream().sorted(Comparator.comparing(o -> o.getRsOrder())).collect(Collectors.toList());
	/*	LinkedHashSet<ReportSection> reportSectionSet = new LinkedHashSet<ReportSection>();
		for (ReportSection reportSection: rsSortedList) {
			reportSectionSet.add(reportSection);
		}
		studyDTO.setReportSections(reportSectionSet); */ 
		studyDTO.setReportSections(new HashSet<ReportSection>(rsSortedList));
		
			return studyDTO;
		}
		return null;
	}
	
	List<ReportSection> rsSortedList = studyDTO.getReportSections().stream().sorted(Comparator.comparing(o -> o.getRsOrder())).collect(Collectors.toList());
	
