cd target
jar xf *.jar BOOT-INF/lib/mysema-commons-lang-0.2.4.jar
jar tf BOOT-INF/lib/mysema-commons-lang-0.2.4.jar | findstr /i "org/apache/commons/lang"
<dependency>
  <groupId>com.querydsl</groupId>
  <artifactId>querydsl-jpa</artifactId>
  <version>5.1.0</version>
  <exclusions>
    <exclusion>
      <groupId>com.mysema.commons</groupId>
      <artifactId>mysema-commons-lang</artifactId>
    </exclusion>
  </exclusions>
</dependency>
mvn -q clean package
jar tf target\*.jar | findstr /i "mysema-commons-lang"
jar tf BOOT-INF/lib/mysema-commons-lang-0.2.4.jar | findstr /i "org/apache/commons/lang"
