The method findAll(Specification<PrmBpBehaviorPatsT>, Pageable) is ambiguous for the type BehaviorPatternsRepository

getting error in getBehaviorPatternsByPage_success method , below is repo class  

package com.optum.fads.pgp.behavior.api.repo;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.JpaSpecificationExecutor;
import org.springframework.data.jpa.repository.Modifying;
import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.query.Param;
import org.springframework.stereotype.Repository;
import org.springframework.transaction.annotation.Transactional;
import org.springframework.data.domain.Pageable;
import com.optum.fads.pgp.behavior.api.domain.PrmBpBehaviorPatsT;

@Transactional
@Repository
public interface BehaviorPatternsRepository extends  JpaRepository <PrmBpBehaviorPatsT, Integer>, JpaSpecificationExecutor<PrmBpBehaviorPatsT>{
		
	@Query(" FROM PrmBpBehaviorPatsT bp  where bp.bpId = :behaviorPatternId ")
	PrmBpBehaviorPatsT getBehaviorPatternById(@Param("behaviorPatternId") Integer behaviorPatternId );
	
	@Query(" FROM PrmBpBehaviorPatsT bp  where lower(bp.behaviorPatternName) = :behaviorPatternName ")
	PrmBpBehaviorPatsT getBehaviorPatternByName(@Param("behaviorPatternName") String behaviorPatternName );
	
	@Query("SELECT MAX(bpId)+1 FROM PrmBpBehaviorPatsT")
	Integer retrieveNextBpId();
	
	@Query(" FROM PrmBpBehaviorPatsT bp" )
	List<PrmBpBehaviorPatsT> getBehaviorPatterns(Pageable pageRequest );
	
	@Query(" FROM PrmBpBehaviorPatsT bp WHERE bp.bpId NOT IN :bpIds" )
	List<PrmBpBehaviorPatsT> getAvailableBehaviorPatterns(@Param("bpIds") List<Integer> bpIds, Pageable pageRequest );
	
	@Query(" FROM PrmBpBehaviorPatsT bp  where lower(bp.behaviorPatternName) like :behaviorPatternName "
			+ "and (lower(bp.uiUserBaseCr.userFullName) like :createdBy or lower(bp.uiUserBaseCr.uiLastName) like :createdBy) "
			+ "and (lower(bp.uiUserBaseUpd.userFullName) like :updatedBy or lower(bp.uiUserBaseUpd.uiLastName) like :updatedBy)" )
	List<PrmBpBehaviorPatsT> getBehaviorPatternsBySearchCrit(@Param("behaviorPatternName") String behaviorPatternName, 
			@Param("createdBy") String createdBy, @Param("updatedBy") String updatedBy, Pageable pageRequest );
	
	// My items
	
	@Query(" FROM PrmBpBehaviorPatsT bp  where lower(bp.behaviorPatternName) like :behaviorPatternName "
			+ "and (lower(bp.uiUserBaseCr.userFullName) like :createdBy or lower(bp.uiUserBaseCr.uiLastName) like :createdBy) "
			+ "and (lower(bp.uiUserBaseUpd.userFullName) like :updatedBy or lower(bp.uiUserBaseUpd.uiLastName) like :updatedBy) " 
			+ "and (lower(bp.uiUserBaseCr.uiSystemId) = :userSystemId or lower(bp.uiUserBaseUpd.uiSystemId) = :userSystemId)")
	List<PrmBpBehaviorPatsT> getMyBehaviorPatternsBySearchCrit(@Param("behaviorPatternName") String behaviorPatternName, 
			@Param("createdBy") String createdBy, @Param("updatedBy") String updatedBy, @Param("userSystemId") String userSystemId,
			Pageable pageRequest );
	
	// Query for special characters in the behavior pattern name
	
	@Query(" FROM PrmBpBehaviorPatsT bp  where lower(bp.behaviorPatternName) like :behaviorPatternName ESCAPE '\\' "
			+ "and (lower(bp.uiUserBaseCr.userFullName) like :createdBy or lower(bp.uiUserBaseCr.uiLastName) like :createdBy) "
			+ "and (lower(bp.uiUserBaseUpd.userFullName) like :updatedBy or lower(bp.uiUserBaseUpd.uiLastName) like :updatedBy) " )
	List<PrmBpBehaviorPatsT> getBehaviorPatternsBySearchCritEsc(@Param("behaviorPatternName") String behaviorPatternName, 
			@Param("createdBy") String createdBy, @Param("updatedBy") String updatedBy, Pageable pageRequest );
	
	// My Items Query for special characters in the behavior pattern name
	
		@Query(" FROM PrmBpBehaviorPatsT bp  where lower(bp.behaviorPatternName) like :behaviorPatternName ESCAPE '\\' "
				+ "and (lower(bp.uiUserBaseCr.userFullName) like :createdBy or lower(bp.uiUserBaseCr.uiLastName) like :createdBy) "
				+ "and (lower(bp.uiUserBaseUpd.userFullName) like :updatedBy or lower(bp.uiUserBaseUpd.uiLastName) like :updatedBy) " 
				+ "and (lower(bp.uiUserBaseCr.uiSystemId) = :userSystemId or lower(bp.uiUserBaseUpd.uiSystemId) = :userSystemId)")
		List<PrmBpBehaviorPatsT> getMyBehaviorPatternsBySearchCritEsc(@Param("behaviorPatternName") String behaviorPatternName, 
				@Param("createdBy") String createdBy, @Param("updatedBy") String updatedBy, @Param("userSystemId") String userSystemId,
				Pageable pageRequest );
		
	// Selected Items
	
	@Query(" FROM PrmBpBehaviorPatsT bp  where lower(bp.behaviorPatternName) like :behaviorPatternName "
			+ "and (lower(bp.uiUserBaseCr.userFullName) like :createdBy or lower(bp.uiUserBaseCr.uiLastName) like :createdBy) "
			+ "and (lower(bp.uiUserBaseUpd.userFullName) like :updatedBy or lower(bp.uiUserBaseUpd.uiLastName) like :updatedBy)"
			+ " and bp.bpId NOT IN :bpIds")
	List<PrmBpBehaviorPatsT> getAvailableBehaviorPatternsBySearchCrit(@Param("behaviorPatternName") String behaviorPatternName, 
			@Param("createdBy") String createdBy, @Param("updatedBy") String updatedBy, @Param("bpIds") List<Integer> bpIds,
			Pageable pageRequest );
	
	// My Items with Selected Items
	
	@Query(" FROM PrmBpBehaviorPatsT bp  where lower(bp.behaviorPatternName) like :behaviorPatternName "
			+ "and (lower(bp.uiUserBaseCr.userFullName) like :createdBy or lower(bp.uiUserBaseCr.uiLastName) like :createdBy) "
			+ "and (lower(bp.uiUserBaseUpd.userFullName) like :updatedBy or lower(bp.uiUserBaseUpd.uiLastName) like :updatedBy) " 
			+ "and (lower(bp.uiUserBaseCr.uiSystemId) = :userSystemId or lower(bp.uiUserBaseUpd.uiSystemId) = :userSystemId)" 
			+ " and bp.bpId NOT IN :bpIds")
	List<PrmBpBehaviorPatsT> getMyAvailableBehaviorPatternsBySearchCrit(@Param("behaviorPatternName") String behaviorPatternName, 
			@Param("createdBy") String createdBy, @Param("updatedBy") String updatedBy,  @Param("userSystemId") String userSystemId,
			@Param("bpIds") List<Integer> bpIds, Pageable pageRequest );
	
	// Query for special characters in the behavior pattern name, with Selected Items
	
	@Query(" FROM PrmBpBehaviorPatsT bp  where lower(bp.behaviorPatternName) like :behaviorPatternName ESCAPE '\\' "
			+ "and (lower(bp.uiUserBaseCr.userFullName) like :createdBy or lower(bp.uiUserBaseCr.uiLastName) like :createdBy) "
			+ "and (lower(bp.uiUserBaseUpd.userFullName) like :updatedBy or lower(bp.uiUserBaseUpd.uiLastName) like :updatedBy)"  
			+ " and bp.bpId NOT IN :bpIds")
	List<PrmBpBehaviorPatsT> getAvailableBehaviorPatternsBySearchCritEsc(@Param("behaviorPatternName") String behaviorPatternName, 
			@Param("createdBy") String createdBy, @Param("updatedBy") String updatedBy, @Param("bpIds") List<Integer> bpIds,
			Pageable pageRequest );
	
	// My Items Query for special characters in the behavior pattern name, with Selected Items
	
	@Query(" FROM PrmBpBehaviorPatsT bp  where lower(bp.behaviorPatternName) like :behaviorPatternName ESCAPE '\\' "
			+ "and (lower(bp.uiUserBaseCr.userFullName) like :createdBy or lower(bp.uiUserBaseCr.uiLastName) like :createdBy) "
			+ "and (lower(bp.uiUserBaseUpd.userFullName) like :updatedBy or lower(bp.uiUserBaseUpd.uiLastName) like :updatedBy) " 
			+ "and (lower(bp.uiUserBaseCr.uiSystemId) = :userSystemId or lower(bp.uiUserBaseUpd.uiSystemId) = :userSystemId)"  
			+ " and bp.bpId NOT IN :bpIds")
	List<PrmBpBehaviorPatsT> getMyAvailableBehaviorPatternsBySearchCritEsc(@Param("behaviorPatternName") String behaviorPatternName, 
			@Param("createdBy") String createdBy, @Param("updatedBy") String updatedBy, @Param("userSystemId") String userSystemId,
			@Param("bpIds") List<Integer> bpIds, Pageable pageRequest );
	
	@Modifying
	@Query("DELETE FROM PrmBpBehaviorPatsT bp  WHERE bp.bpId = :behaviorpatternId") 
	int deleteBehaviorPattern(@Param("behaviorpatternId") Integer behaviorpatternId  );
	
	@Query(" FROM PrmBpBehaviorPatsT bp WHERE bp.bpId IN :bpIds") 
	List<PrmBpBehaviorPatsT> getBehaviorPatternsByIds(@Param("bpIds") List<Integer> bpIds);
	
	@Query(" FROM PrmBpBehaviorPatsT bp WHERE lower(bp.behaviorPatternName) like :behaviorPatternName "
			+ "AND bp.uiUserBaseCr.uiUserId = :loginId")
	List<PrmBpBehaviorPatsT> getPatternsByLogin(@Param("behaviorPatternName") String behaviorPatternName, 
			@Param("loginId") String loginId, Pageable pageRequest );
	
	@Query(" FROM PrmBpBehaviorPatsT bp WHERE lower(bp.behaviorPatternName) like :behaviorPatternName ESCAPE '\\' "
			+ "AND bp.uiUserBaseCr.uiUserId = :loginId")
	List<PrmBpBehaviorPatsT> getPatternsByLoginEsc(@Param("behaviorPatternName") String behaviorPatternName, 
			@Param("loginId") String loginId, Pageable pageRequest );
		
}
