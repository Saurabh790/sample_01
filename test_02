mvn -q dependency:tree -Dincludes=com.azure:azure-identity
mvn help:effective-pom | findstr /i "azure-identity"
mvn -q dependency:tree | findstr /i "azure-identity"
jar tf target/*.jar | findstr /i "azure-identity"
dir target\lib | findstr /i azure-identity



mvn dependency:tree -Dverbose -Dincludes=org.apache.commons:commons-lang3
mvn dependency:tree -Dverbose -Dincludes=commons-lang:commons-lang
jar tf target\*.jar | findstr /i "commons-lang3"
jar tf target\*.jar | findstr /i "commons-lang"
jar tf target\*.jar | findstr /i "commons-lang"
