package com.optum.fads.pgp.behavior.api;

import static org.junit.jupiter.api.Assertions.assertDoesNotThrow;

import org.junit.jupiter.api.Test;

class BehaviorPatternApiApplicationTest {

    @Test
    void mainMethod_runsWithoutException() {
        // Just verify that the main method can be called without throwing
        assertDoesNotThrow(() ->
                BehaviorPatternApiApplication.main(new String[] {})
        );
    }
}
