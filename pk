mvn -U -e clean package
mvn -q dependency:tree -Dincludes=ch.qos.logback:logback-core,ch.qos.logback:logback-classic
jar tf target/*.jar | findstr /i logback-core
jar tf target\fads-pgp-reportsection-api-1.0.0.jar | findstr /i logback-core
dir target\lib | findstr /i logback
Get-ChildItem target\*.jar | % { jar tf $_.FullName } | Select-String -Pattern "logback-core" -CaseSensitive:$false
jar tf target\fads-pgp-reportsection-api-1.0.0.jar | findstr /i "BOOT-INF/lib/logback-core"

