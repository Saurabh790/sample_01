mvn -q dependency:tree -Dincludes=org.apache.commons:commons-lang3,commons-lang:commons-lang
mvn dependency:tree -Dincludes=org.apache.commons:commons-lang3,commons-lang:commons-lang
mvn dependency:tree -Dverbose -Dincludes=org.apache.commons:commons-lang3,commons-lang:commons-lang
mvn dependency:tree -Dverbose | Select-String "commons-lang3|commons-lang:"
mvn dependency:tree -Dverbose | findstr /I "commons-lang3 commons-lang:"
mvn dependency:tree -Dverbose | egrep "commons-lang3|commons-lang:"
mvn dependency:list -DincludeArtifactIds=commons-lang3,commons-lang -DincludeGroupIds=org.apache.commons,commons-lang
mvn -q help:evaluate -Dexpression=project.artifactId -DforceStdout
