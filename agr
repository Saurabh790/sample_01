mvn -q dependency:tree -Dincludes=org.apache.commons:commons-lang3,commons-lang:commons-lang -Dverbose
mvn -q dependency:tree -Dverbose -Dincludes=*:*commons-lang*

mvn dependency:tree -Dverbose | findstr /i "commons-lang lang3"
mvn dependency:tree -Dverbose | Select-String -Pattern "commons-lang","lang3" -CaseSensitive:$false
mvn dependency:tree -Dverbose | grep -iE "commons-lang|lang3"
mvn dependency:list -DincludeArtifactIds=commons-lang3,commons-lang -DoutputAbsoluteArtifactFilename=true
mvn -q dependency:tree -Dverbose -Dscope=test | findstr /i "commons-lang3"
mvn -q dependency:list -DincludeArtifactIds=commons-lang3 -DoutputAbsoluteArtifactFilename=true
jar tf target\*.jar | findstr /i "commons-lang"

