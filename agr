# 1) Check if commons-lang3 exists anywhere AND which versions
mvn -DskipTests dependency:tree -Dverbose -Dincludes=org.apache.commons:commons-lang3

# 2) Check if old commons-lang (2.x) exists anywhere
mvn -DskipTests dependency:tree -Dverbose -Dincludes=commons-lang:commons-lang


mvn -DskipTests dependency:list -DincludeArtifactIds=commons-lang3 -DincludeGroupIds=org.apache.commons


jar tf target/*.jar | findstr /i "commons-lang3"
