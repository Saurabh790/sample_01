package com.optum.fads.authorization.api.entity;

import jakarta.persistence.Entity;
import jakarta.persistence.Table;

/**
 * Concrete entity for SE_SUR_GRP_MOD_ACCESS
 */
@Entity
@Table(name = "SE_SUR_GRP_MOD_ACCESS")
public class SeSurGrpModAccess extends AbstractSeSurGrpModAccess implements java.io.Serializable {

    public SeSurGrpModAccess() {}

    public SeSurGrpModAccess(SeSurGrpModAccessId id, SeSurGrp seSurGrp, SeSurModule seSurModule) {
        super(id, seSurGrp, seSurModule);
    }

    public SeSurGrpModAccess(SeSurGrpModAccessId id, SeSurAccess seSurAccess,
                             SeSurGrp seSurGrp, SeSurModule seSurModule) {
        super(id, seSurAccess, seSurGrp, seSurModule);
    }
}
