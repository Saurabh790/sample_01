SELECT SUR_MODULE_ID, COUNT(*) 
FROM SE_SUR_MODULE 
GROUP BY SUR_MODULE_ID 
HAVING COUNT(*) > 1;

-- duplicates in access:
SELECT SUR_ACCESS_ID, COUNT(*) 
FROM SE_SUR_ACCESS 
GROUP BY SUR_ACCESS_ID 
HAVING COUNT(*) > 1;

-- duplicates in the link for one group (replace :grp):
SELECT SUR_GRP_ID, SUR_MODULE_ID, COUNT(*) 
FROM SE_SUR_GRP_MOD_ACCESS
WHERE SUR_GRP_ID = :grp
GROUP BY SUR_GRP_ID, SUR_MODULE_ID
HAVING COUNT(*) > 1;
