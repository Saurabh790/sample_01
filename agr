package com.optum.fads.authorization.api.entity;

import jakarta.persistence.AttributeOverride;
import jakarta.persistence.AttributeOverrides;
import jakarta.persistence.Column;
import jakarta.persistence.EmbeddedId;
import jakarta.persistence.FetchType;
import jakarta.persistence.JoinColumn;
import jakarta.persistence.ManyToOne;
import jakarta.persistence.MappedSuperclass;

/**
 * Base mapping for SE_SUR_GRP_MOD_ACCESS
 */
@MappedSuperclass
public abstract class AbstractSeSurGrpModAccess implements java.io.Serializable {

    private SeSurGrpModAccessId id;
    private SeSurAccess seSurAccess;   // via SUR_GRP_MOD_ACCESS -> SE_SUR_ACCESS.SUR_ACCESS_ID
    private SeSurGrp seSurGrp;         // via SUR_GRP_ID
    private SeSurModule seSurModule;   // via SUR_MODULE_ID

    public AbstractSeSurGrpModAccess() {}

    public AbstractSeSurGrpModAccess(SeSurGrpModAccessId id, SeSurGrp seSurGrp, SeSurModule seSurModule) {
        this.id = id;
        this.seSurGrp = seSurGrp;
        this.seSurModule = seSurModule;
    }

    public AbstractSeSurGrpModAccess(SeSurGrpModAccessId id, SeSurAccess seSurAccess,
                                     SeSurGrp seSurGrp, SeSurModule seSurModule) {
        this.id = id;
        this.seSurAccess = seSurAccess;
        this.seSurGrp = seSurGrp;
        this.seSurModule = seSurModule;
    }

    @EmbeddedId
    @AttributeOverrides({
        @AttributeOverride(name = "surGrpId",    column = @Column(name = "SUR_GRP_ID",    nullable = false)),
        @AttributeOverride(name = "surModuleId", column = @Column(name = "SUR_MODULE_ID", nullable = false))
    })
    public SeSurGrpModAccessId getId() { return id; }
    public void setId(SeSurGrpModAccessId id) { this.id = id; }

    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(
        name = "SUR_GRP_MOD_ACCESS",           // column present in your table
        referencedColumnName = "SUR_ACCESS_ID",// PK column of SE_SUR_ACCESS
        nullable = true
    )
    public SeSurAccess getSeSurAccess() { return seSurAccess; }
    public void setSeSurAccess(SeSurAccess seSurAccess) { this.seSurAccess = seSurAccess; }

    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name = "SUR_GRP_ID", nullable = false, insertable = false, updatable = false)
    public SeSurGrp getSeSurGrp() { return seSurGrp; }
    public void setSeSurGrp(SeSurGrp seSurGrp) { this.seSurGrp = seSurGrp; }

    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name = "SUR_MODULE_ID", nullable = false, insertable = false, updatable = false)
    public SeSurModule getSeSurModule() { return seSurModule; }
    public void setSeSurModule(SeSurModule seSurModule) { this.seSurModule = seSurModule; }
}
