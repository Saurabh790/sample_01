✅ Corrected AbstractSeSurGrpModAccess.java
package com.optum.fads.authorization.api.entity;

import jakarta.persistence.AttributeOverride;
import jakarta.persistence.AttributeOverrides;
import jakarta.persistence.Column;
import jakarta.persistence.EmbeddedId;
import jakarta.persistence.FetchType;
import jakarta.persistence.JoinColumn;
import jakarta.persistence.ManyToOne;
import jakarta.persistence.MappedSuperclass;

@MappedSuperclass
public abstract class AbstractSeSurGrpModAccess implements java.io.Serializable {

    private SeSurGrpModAccessId id;
    private SeSurAccess seSurAccess;
    private SeSurGrp seSurGrp;
    private SeSurModule seSurModule;

    public AbstractSeSurGrpModAccess() {
    }

    public AbstractSeSurGrpModAccess(SeSurGrpModAccessId id, SeSurGrp seSurGrp, SeSurModule seSurModule) {
        this.id = id;
        this.seSurGrp = seSurGrp;
        this.seSurModule = seSurModule;
    }

    public AbstractSeSurGrpModAccess(SeSurGrpModAccessId id, SeSurAccess seSurAccess, SeSurGrp seSurGrp,
                                     SeSurModule seSurModule) {
        this.id = id;
        this.seSurAccess = seSurAccess;
        this.seSurGrp = seSurGrp;
        this.seSurModule = seSurModule;
    }

    @EmbeddedId
    @AttributeOverrides({
            @AttributeOverride(name = "surGrpId", column = @Column(name = "SUR_GRP_ID", nullable = false)),
            @AttributeOverride(name = "surModuleId", column = @Column(name = "SUR_MODULE_ID", nullable = false))
    })
    public SeSurGrpModAccessId getId() {
        return this.id;
    }

    public void setId(SeSurGrpModAccessId id) {
        this.id = id;
    }

    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name = "SUR_ACCESS_TYPE", nullable = false)
    public SeSurAccess getSeSurAccess() {
        return this.seSurAccess;
    }

    public void setSeSurAccess(SeSurAccess seSurAccess) {
        this.seSurAccess = seSurAccess;
    }

    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name = "SUR_GRP_ID", nullable = false, insertable = false, updatable = false)
    public SeSurGrp getSeSurGrp() {
        return this.seSurGrp;
    }

    public void setSeSurGrp(SeSurGrp seSurGrp) {
        this.seSurGrp = seSurGrp;
    }

    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name = "SUR_MODULE_ID", nullable = false, insertable = false, updatable = false)
    public SeSurModule getSeSurModule() {
        return this.seSurModule;
    }

    public void setSeSurModule(SeSurModule seSurModule) {
        this.seSurModule = seSurModule;
    }
}

✅ Concrete SeSurGrpModAccess.java
package com.optum.fads.authorization.api.entity;

import jakarta.persistence.Entity;
import jakarta.persistence.Table;

@Entity
@Table(name = "SE_SUR_GRP_MOD_ACCESS")
public class SeSurGrpModAccess extends AbstractSeSurGrpModAccess implements java.io.Serializable {

    public SeSurGrpModAccess() {
    }

    public SeSurGrpModAccess(SeSurGrpModAccessId id, SeSurGrp seSurGrp, SeSurModule seSurModule) {
        super(id, seSurGrp, seSurModule);
    }

    public SeSurGrpModAccess(SeSurGrpModAccessId id, SeSurAccess seSurAccess, SeSurGrp seSurGrp,
                             SeSurModule seSurModule) {
        super(id, seSurAccess, seSurGrp, seSurModule);
    }
}
