mvn -q dependency:tree -Dincludes=io.netty:netty-codec-http,io.netty:netty-handler,io.netty:netty-codec
mvn dependency:tree -Dincludes=io.netty -Dverbose -Dscope=runtime
mvn help:effective-pom > effective-pom.xml
jar tf target/*.jar | findstr /i "io/netty"
jar tf target/*.jar | findstr /i "BOOT-INF/lib" | findstr /i "netty"
